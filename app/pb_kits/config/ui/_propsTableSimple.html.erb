<% component_props ||= nil %>

<div class="pb--propsTable">
  <table class="pb--propsTable--table">
    <thead>
      <tr>
        <th>Prop</th>
        <th>Value</th>
      </tr>
    </thead>
    <tbody>
      <% if( !component_props.nil? ) %>
        <% component_props[:props].each_with_index do |(key,value),index| %>
          <tr>
            <td><%= key %></td>
            <td><%= value %></td>
          </tr>
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
