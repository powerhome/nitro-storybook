<%
  props ||= {}
  wrapperclass = props.fetch(:wrapperclass, 'kit-btn-wrapper')
  componentclass = props.fetch(:componentclass, 'kit-btn')
  text = props.fetch(:text, 'Button')
%>

<div class="<%= wrapperclass %>">
    <button class="<%= componentclass %>">
        <span><%= text %></span>
    </button>
</div>
