<%= pb_rails("heading", props: { text: pb_title(@kit), tag: "h1", size: "1" }) %>

<div class="pb--kit-type-nav">
  <a href="#{kit_show_rails_path(@kit)}" class="">Rails</a>
  <a href="#{kit_show_reacts_path(@kit)}" class="">React</a>

  <% if(action_name == "kit_show_rails") %>
    <%= pb_kit(@kit) %>
  <% else %>
    <%= pb_kit(@kit, "react") %>
  <% end %>

  <%= pb_rails_with_docs("card", props: {name: "Kits"}) do -%>
    <span>xxxx</span>
    <h3>ddd</h3>
  <% end -%>
</div>
